#include <bits/stdc++.h> 
void insertionSort(int n, vector<int> &arr){
 for (int i =1; i<n; i++){
     int current = arr[i];
     int prev= i-1;
     while(prev>=0){
         if(arr[prev]> current){
             arr[prev+1]= arr[prev];
         }
         else{
             break;
         }
         prev--;
     }
     arr[prev+1]=current;

 }

    // Write your code here.
}
