# include <iostream>
# include <vector>
using namespace std;
void merge(int arr1[], int n, int arr2[], int m , int arr3[]){
    int i=0,j=0,k=0;
    while(i<n && j<m){
        if (arr1[i]< arr2[j]){
            arr3[k]=arr1[i];
            i++;
        }
        else{
            arr3[k]= arr2[j];
            j++;
        }
        k++;

    }
    while (i<n)
    { arr3[k]= arr1[i];
    i++;
k++;}
    while(j<m){
        arr3[k]= arr2[j];
        j++;
        k++;
    }
    
    
}

  void print(int ans[], int n){
    for(int i =0; i<n; i++){
        cout<< ans[i]<< " ";
    }
  }
  int main(){
    int arr1[5]={10,20,30,40,50};
    int arr2[4]= {15,25,35,45};
    int arr3[9];
    merge(arr1,5,arr2,4,arr3);
    print(arr3,9);
    return 0;

  }
  
  
   
   

 




 
