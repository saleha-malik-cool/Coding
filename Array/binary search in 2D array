# include <iostream>
using namespace std;

int binary(int arr[][4] , int  target, int row, int col){
    int s = 0;
    int e = (row* col )-1;
    int mid = s +(e-s)/2;
while( s<=e){
    int matrix= arr[mid/col][mid%col];
    if ( target == matrix ){
        return  1;}
     if ( target < matrix ){
            e = mid -1;
        }
        else{
            s = mid +1;
        }
        mid =s +(e-s)/2;
    }
    return 0;
}

int main (){
    int arr[3][4]= {1,2,3,4,5,6,7,8,910,11,12};
    int ans = binary(arr, 6, 3,4);
    cout<< ans;
}
