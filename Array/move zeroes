# include <iostream>
# include <vector>
using namespace std;
void movezeroes(int arr[], int n){
    int i =0;
    for(int j =0; j< n; j++){
        if(arr[j]!= 0){
            swap(arr[i], arr[j]);
            i++;
        }
    }
}
void print(int arr[], int n){
    for(int i=0; i<n; i++){
        cout<<arr[i]<<" ";

    }
}
int main(){
    int arr[7]={0,1,0,2,3,0,0};
    movezeroes(arr,7);
    print(arr,7);
    return 0;
}
