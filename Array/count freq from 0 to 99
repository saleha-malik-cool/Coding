# include <iostream>
using namespace std;
int main(){
    int arr []= {1,5,3,1,5,7,99,5,3};
    int n = sizeof (arr)/ sizeof (arr[0]);
    int freq[100]= {0};
    for( int i =0; i<n; i++){
        freq[arr[i]]++;
    }
    for(int i =0; i<100; i++){
        if(freq[i]>0){
            cout<< i << " appears " << freq[i] << " times " << endl;
        }
    }
return 0;
}
