# include <iostream>
# include <vector>
using namespace std;

 bool issorted(vector <int> &arr, int n){
    for(int i=0 ; i<n-1; i++){
        if (arr[i]<=arr[i+1]){

        }
        else{ 
            return false;
        }
    }
    return true;
   }
   int main(){
    vector <int> arr= {2,3,4,8,78,667,5666,6666};
   if(issorted(arr, arr.size())){
    cout<<"array is sorted"<<endl;
   }
   else{
    cout<<"array is not sorted"<<endl;
   }
   
   }
