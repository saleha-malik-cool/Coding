# include <iostream>
using namespace std;
int main(){
  int arr[10]= {10,20,30,40,50};
  int n =5;
  int pos, element;
  cout<<" enter the value of pos: "<< endl;
  cin>> pos;
  cout<<" enter the elment: "<< endl;
  cin>>element;
  for(int i = n; i>pos; i--){
    arr[i]=arr[i-1];

  }
  arr[pos]= element;
  n++;
  for(int i=0; i<n; i++){
    cout<<arr[i]<<" ";
  }



}
 
