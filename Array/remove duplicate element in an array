# include <iostream>
# include <vector>
using namespace std;
int removeduplicate(vector <int> &arr, int n){
    int i=0;
    for(int j=1; j<n; j++){
        if(arr[j]!= arr[i]){
            arr[i+1]= arr[j];
            i++;
        }
    }
    return i+1;
}
int main(){
   vector <int> arr= {1,1,2,2,2,3,3,4};
   int  solve = removeduplicate(arr,arr.size());
 for (int k=0; k<solve; k++){
    cout<< arr[k]<<" ";
 }

}

 
