# include <iostream>
# include <vector>
using namespace std;

    int secondlargest(vector<int>&arr, int n){
        int largest=arr[0];
        int slargest = -1;
        for(int i =1; i<n; i++){
            if(arr[i]>largest){
                slargest= largest;
                largest= arr[i];
            }
            else if(arr[i]< largest && arr[i]>slargest){
                slargest=arr[i];
            }
        }
        return slargest;}

    int main(){
    vector <int> arr={2,3,4,1,5};
int l = secondlargest(arr,arr.size());
cout<<l;

}
